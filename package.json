{
	"name": "visual-code-glua-ide",
	"displayName": "Visual Code GLua IDE",
	"description": "An advanced Garry's Mod lua IDE",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/.glua"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.currentTime",
				"title": "Display Current Time"
			}
		],
		"languages": [
			{
				"id": "glua",
				"extensions": [
					".lua"
				],
				"aliases": [
					"Garry's Mod Lua"
				],
				"configuration": "./language-configuration.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/he": "^1.1.0",
		"@types/luaparse": "0.0.29",
		"@types/request-promise": "^4.1.45",
		"he": "^1.2.0",
		"luaparse": "git://github.com/Donkie/luaparse.git#glua",
		"request": "^2.88.0",
		"request-promise": "^4.2.5"
	}
}
